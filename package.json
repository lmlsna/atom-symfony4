{
  "name": "atom-symfony4",
  "main": "./lib/atom-symfony4",
  "version": "0.4.0",
  "description": "Symfony4 tools and auto-completion",
  "keywords": [
    "php",
    "symfony4",
    "symfony",
    "atom",
    "autocomplete"
  ],
  "repository": "https://github.com/lmlsna/atom-symfony4",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "package-deps": [
    "atom-autocomplete-php"
  ],
  "dependencies": {
    "fuzzaldrin": "^2.1.0"
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "getProvider"
      }
    },
    "autocomplete.php.plugin": {
      "versions": {
        "1.0.0": "getPhpPlugin"
      }
    }
  },
  "consumedServices": {
    "php.autocomplete.tools": {
      "versions": {
        "0.16.0": "getAutocompleteTools"
      }
    }
  }
}
